public class Entity{
  public double x;
  public double y;
  public double dx;
  public double dy;
  public double radius;
  
  public double getx(){
    return x;
  }
  
  public double gety(){
    return y;
  }
  
  public double getdx(){
    return dx;
  }
  
  public double getdy(){
    return dy;
  }
  
  public double getradius(){
    return radius;
  }
}
